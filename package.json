{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "homepage": "/react-template",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.59",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "axios": "^1.5.1",
    "lodash-es": "^4.17.21",
    "msw": "^1.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.17.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "prebuild": "npm run format && npm run lint",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "preanalyze": "npm run build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint": "eslint src --ext .ts,.tsx,.mts,.js,.jsx,.mjs,.vue --max-warnings 0",
    "format": "prettier --write ."
  },
  "lint-staged": {
    "*.{ts,tsx,mts,js,jsx,mjs,vue}": [
      "eslint --max-warnings 0",
      "prettier --write --ignore-unknown"
    ],
    "*.{html,htm,css,less,scss,json,md,yaml,yml}": "prettier --write --ignore-unknown"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/lodash-es": "^4.17.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.33.2",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.3.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
